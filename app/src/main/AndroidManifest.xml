<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigation3_requiem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/saadoeyer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/saadoeyer"
        android:supportsRtl="true"
        android:theme="@style/Theme.Navigation3_Requiem">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Navigation3_Requiem.NoActionBar">
        </activity>
        <activity
            android:name=".Gacha"
            android:label="@string/gacha"
            android:theme="@style/Theme.Navigation3_Requiem.NoActionBar">
        </activity>
        <activity
            android:name=".Splash"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>